<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="CAT200.ErrorWindowController">
    <children>
        <Button fx:id="OkayButton" layoutX="260.0" layoutY="322.0" mnemonicParsing="false" onKeyPressed="#close_window"
                onMouseClicked="#close_window" text="Okay">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Label layoutX="87.0" layoutY="35.0" prefHeight="92.0" prefWidth="162.0" text="ERROR">
            <font>
                <Font name="Angkor Regular" size="40.0"/>
            </font>
        </Label>
        <TextArea fx:id="error_text" editable="false" layoutX="87.0" layoutY="127.0" prefHeight="146.0"
                  prefWidth="468.0"/>
    </children>
    <stylesheets>
        <URL value="@script.css"/>
    </stylesheets>
</AnchorPane>
